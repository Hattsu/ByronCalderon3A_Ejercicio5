using System;
using System.Collections.Generic;
using System.Text;
namespace ByronCalderon3A_Ejercicio5
{
    public class Suma
    {
        private int[] Numero { set; get; }
        private int Suma { get; set; }
        public void Calcular()
        {
        Numero = new int[8] { 5, 6, 7, 8, 9, 5, 6, 5 } ;
            for (int i = 0; i <= 7; i++)
            {
                Console.WriteLine("Los nÃºmeros son: "+Numero[i]);
                Suma = Suma+ Numero[i];
            }
            Console.WriteLine("Suma total de todos los numeros ingresados: " +Suma);
        }
    }
}
